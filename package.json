{
    "name": "doz",
    "version": "3.16.0",
    "description": "A JavaScript framework for building UI, almost like writing in VanillaJS.",
    "main": "dist/doz.js",
    "module": "dist/esm/index.js",
    "sideEffects": false,
    "typings": "types/index.d.ts",
    "scripts": {
        "release:before": "npm run-script test && npm run-script doc && git commit -a -m DOC",
        "release:major": "webpack --env.major && npm run-script release:publish",
        "release:minor": "webpack --env.minor && npm run-script release:publish",
        "release:patch": "webpack --env.patch && npm run-script release:publish",
        "release:publish": "git commit -a -m BUILD && version-to-tag.sh && npm publish",
        "to-esm": "node to-esm.js",
        "esm-minify": "node esm-minify.js",
        "buld.and.push": "npm run-script build && git commit -a -m BUILD && git push",
        "watch": "webpack --watch --progress --debug",
        "build": "npm run-script to-esm && webpack --progress",
        "test": "mocha test",
        "test:hmr-watch": "parcel watch test/html/HMR/index.js --out-file bundle --no-cache --out-dir test/html/HMR/public/dist",
        "test:hmr-build": "parcel build test/html/HMR/index.js --out-file bundle --no-cache --out-dir test/html/HMR/public/dist --public-url ./"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dozjs/doz.git"
    },
    "keywords": [
        "front-end",
        "html",
        "dom",
        "javascript",
        "framework",
        "ui",
        "virtual dom",
        "v-dom",
        "component",
        "plugin",
        "mixin",
        "proxy",
        "hooks",
        "pre-render",
        "server side rendering",
        "ssr",
        "component-based",
        "webcomponent",
        "tagged-template-literals",
        "template-literals"
    ],
    "author": "Fabio Ricali",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/dozjs/doz/issues"
    },
    "homepage": "https://github.com/dozjs/doz#readme",
    "dependencies": {},
    "devDependencies": {
        "@babel/core": "^7.16.10",
        "@babel/plugin-proposal-optional-chaining": "^7.16.7",
        "@babel/preset-env": "^7.16.10",
        "@babel/register": "^7.16.9",
        "autoprefixer": "^9.8.8",
        "babel-loader": "^8.2.3",
        "babel-plugin-loop-optimizer": "^1.4.1",
        "bejs": "^2.0.1",
        "chai": "^4.1.2",
        "cjstoesm": "^1.1.4",
        "compression-webpack-plugin": "^1.1.11",
        "doz-router": "^1.8.1",
        "faster.js": "^1.1.1",
        "jsdom": "^11.6.2",
        "jsdom-global": "^3.0.2",
        "mocha": "^5.0.4",
        "postcss-modules": "^1.3.0",
        "terser": "^5.7.1",
        "typescript": "^3.1.1",
        "unminified-webpack-plugin": "^1.4.2",
        "webpack": "^3.10.0",
        "webpack-auto-inject-version": "^1.0.0"
    }
}